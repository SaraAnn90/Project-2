<!-- Product -->
<section class="content-section" id="productDetail">
    <div class="container">
        {{#with product}}
        <div class="content-section-heading text-center">
            <h2 class="mb-5">{{dataValues.brandName}}</h2>
        </div>
        <div class="row row-cols-1 row-cols-md-2">
            <p class="card-text">{{dataValues.foodName}}</p>
            <p class="card-text">{{dataValues.petType}}</p>
            <p class="card-text">{{dataValues.foodType}}</p>
            <p class="card-text">{{dataValues.ingred001}}</p>
            <p class="card-text">{{dataValues.ingred002}}</p>
            <p class="card-text">{{dataValues.ingred003}}</p>
            <p class="card-text">{{dataValues.ingred004}}</p>
            <p class="card-text">{{dataValues.ingred005}}</p>
        </div>
        {{/with}}
    </div>
</section>


